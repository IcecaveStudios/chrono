{
    "name": "icecave/chrono",
    "description": "PHP date and time abstractions that are decoupled from the system clock.",
    "keywords": ["time", "date", "clock", "timer", "chrono", "iso"],
    "homepage": "https://github.com/IcecaveStudios/chrono",
    "license": "MIT",
    "authors": [
        {
            "name": "James Harris",
            "email": "james.harris@icecave.com.au"
        }
    ],
    "require": {
        "php": ">=5.3.3"
    },
    "require-dev": {
        "eloquent/typhoon": "~0.8",
        "eloquent/liberator": "~1",
        "icecave/testing": "~2",
        "icecave/woodhouse": "*"
    },
    "autoload": {
        "psr-0": {
            "Icecave\\Chrono": "lib",
            "Icecave\\Chrono\\TypeCheck": "lib-typhoon"
        }
    },
    "extra": {
        "typhoon": {
            "output-path": "lib-typhoon",
            "validator-namespace": "Icecave\\Chrono\\TypeCheck",
            "use-native-callable": false
        }
    }
}
