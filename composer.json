{
    "name": "icecave/chrono",
    "type": "library",
    "description": "Dates, times, clocks, and timers.",
    "keywords": ["time", "date", "clock", "timer", "chrono"],
    "homepage": "https://github.com/IcecaveStudios/chrono",
    "license": "MIT",
    "authors": [
        {
            "name": "James Harris",
            "email": "james.harris@icecave.com.au"
        }
    ],
    "require": {
        "php": ">=5.3.0"
    },
    "require-dev": {
        "icecave/testing": "1.*",
        "eloquent/typhoon": "0.6.*"
    },
    "autoload": {
        "psr-0": {
            "Icecave\\Chrono": "lib",
            "Icecave\\Chrono\\TypeCheck": "lib-typhoon"
        }
    },
    "extra": {
        "typhoon": {
            "output-path": "lib-typhoon",
            "validator-namespace": "Icecave\\Chrono\\TypeCheck",
            "use-native-callable": false
        }
    }
}
